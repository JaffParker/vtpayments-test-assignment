{
  "name": "vtpayments-terminal-api",
  "version": "0.0.0",
  "description": "Virtual terminal API server",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc --skipLibCheck",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "INTERP=js node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/graphql": "^6.0.5",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/typeorm": "^6.0.0",
    "apollo-server-express": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^1.30.1",
    "ejs": "^2.6.1",
    "email-templates": "^5.0.4",
    "file-type": "^10.7.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.21",
    "mysql": "^2.16.0",
    "nestjs-config": "^1.2.9-alpha",
    "nodemailer": "^6.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.10",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.1.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^2.6.3",
    "@types/email-templates": "^3.5.1",
    "@types/express": "^4.16.0",
    "@types/file-type": "^10.6.0",
    "@types/graphql": "^14.0.4",
    "@types/graphql-type-json": "^0.1.3",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.120",
    "@types/mime-types": "^2.1.0",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.7.1",
    "@types/nodemailer": "^4.6.7",
    "@types/supertest": "^2.0.5",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
